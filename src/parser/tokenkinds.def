// This file is here as a definition of all tokens together
// with their "name" -- their real-looking form.

// There are three macros:
// * TOKEN(kind, name) for general tokens
// * KEYWORD(kind, name) for keywords
// * LITERAL(kind, name) for literals

// For example usage, see files `token.hpp` and `token.cpp`
// For more details, see http://en.wikibooks.org/wiki/C_Programming/Preprocessor#X-Macros

TOKEN(EndOfFile, "<EOF>")
TOKEN(LParen, ")")
TOKEN(RParen, "(")
TOKEN(LBrace, "{")
TOKEN(RBrace, "}")
TOKEN(LBracket, "[")
TOKEN(RBracket, "]")
TOKEN(Ampersand, "&")
TOKEN(AmpersandEq, "&=")
TOKEN(At, "@")
TOKEN(Backslash, "\\")
TOKEN(Bang, "!")
TOKEN(BangEq, "!=")
TOKEN(Caret, "^")
TOKEN(Colon, ":")
TOKEN(ColonColon, "::")
TOKEN(Comma, ",")
TOKEN(Dot, ".")
TOKEN(DotDot, "..")
TOKEN(DotDotDot, "...")
TOKEN(Eq, "=")
TOKEN(EqEq, "==")
TOKEN(EqGreater, "=>")
TOKEN(Greater, ">")
TOKEN(GreaterEq, ">=")
TOKEN(GreaterGreater, ">>")
TOKEN(GreaterGreaterEq, ">>=")
TOKEN(Hash, "#")
TOKEN(Less, "<")
TOKEN(LessEq, "<=")
TOKEN(LessLess, "<<")
TOKEN(LessLessEq, "<<=")
TOKEN(Minus, "-")
TOKEN(MinusEq, "-=")
TOKEN(MinusGreater, "->")
TOKEN(Percent, "%")
TOKEN(PercentEq, "%=")
TOKEN(PercentPercent, "%%")
TOKEN(PercentPercentEq, "%%=")
TOKEN(Pipe, "|")
TOKEN(PipeEq, "|=")
TOKEN(Plus, "+")
TOKEN(PlusEq, "+=")
TOKEN(PlusPlus, "++")
TOKEN(Question, "?")
TOKEN(QuestionEq, "?=")
TOKEN(Semicolon, ";")
TOKEN(Slash, "/")
TOKEN(SlashEq, "/=")
TOKEN(Star, "*")
TOKEN(StarEq, "*=")
TOKEN(StarStar, "**")
TOKEN(Tilde, "~")
TOKEN(TildeEq, "~=")
TOKEN(Identifier, "<identifier>")
TOKEN(LineComment, "<line comment>")
TOKEN(DocComment, "<doc comment>")

KEYWORD(Align, "align")
KEYWORD(And, "and")
KEYWORD(Asm, "asm")
KEYWORD(Break, "break")
KEYWORD(Catch, "catch")
KEYWORD(Const, "const")
KEYWORD(Continue, "continue")
KEYWORD(Defer, "defer")
KEYWORD(Else, "else")
KEYWORD(Enum, "enum")
KEYWORD(Error, "error")
KEYWORD(Export, "export")
KEYWORD(Extern, "extern")
KEYWORD(False, "false")
KEYWORD(Fn, "fn")
KEYWORD(For, "for")
KEYWORD(If, "if")
KEYWORD(In, "in")
KEYWORD(Match, "match")
KEYWORD(Nil, "nil")
KEYWORD(Not, "not")
KEYWORD(Or, "or")
KEYWORD(Priv, "priv")
KEYWORD(Pub, "pub")
KEYWORD(Pure, "pure")
KEYWORD(Return, "return")
KEYWORD(Struct, "struct")
KEYWORD(This, "this")
KEYWORD(True, "true")
KEYWORD(Try, "try")
KEYWORD(Undefined, "undefined")
KEYWORD(Union, "union")
KEYWORD(Use, "use")
KEYWORD(Var, "var")
KEYWORD(Volatile, "volatile")
KEYWORD(While, "while")

LITERAL(Char, "<char>")
LITERAL(Float, "<float>")
LITERAL(Integer, "<integer>")
LITERAL(CRawString, "<raw cstring>")
LITERAL(CString, "<cstring>")
LITERAL(RawString, "<raw string>")
LITERAL(String, "<string>")
